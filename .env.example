POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=negatiview

#Redis
REDIS_PASSWORD=
REDIS_URL=redis://127.0.0.1:6379/

#Kafka
# KRaft settings
KAFKA_CFG_NODE_ID=0
KAFKA_CFG_PROCESS_ROLES=controller,broker
